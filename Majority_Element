Majority Element
class Solution {
public:
    int majorityElement(vector<int>& nums) {
      int count=0,candidate=0;
      for(int el:nums){
        if(count==0){
            candidate=el;
        }

        if(el==candidate){
            count++;
        }else{
            count--;
        }
      }
      return candidate; 
    }
};
